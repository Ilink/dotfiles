[push]
    default = simple

[credential]
    helper = cache --timeout=3600

[core]
    excludesfile = ~/.gitignore_global
    editor = vim
    pager = less -+$LESS -R

[merge]
    tool = vimdiff
    # conflictstyle = diff3

[mergetool]
    prompt = false

